<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sun.blog.mapper.TUserMapper">

    <select id="login" resultType="com.sun.blog.entity.TUser">
        select*from t_user where username=#{username} and password=#{password}
    </select>
    <select id="selectAll" resultType="com.sun.blog.entity.TUser">
        select *from t_user;
    </select>

</mapper>
